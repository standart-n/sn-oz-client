{
	"name": "sn build",
	"root": "../",
	"output": "script/sn.lmd.js",
	"modules": {
		"_": {
			"path": 				"bower_components/underscore/underscore-min.js",
			"exports": 				"_"
		},
		"underscore": 				"@_",
		"$": {
			"path": 				"bower_components/jquery/jquery.min.js",
			"exports": 				"jQuery"
		},
		"SockJS": {
			"path":					"bower_components/sockjs-client/sockjs.min.js",
			"exports":				"SockJS"
		},
		"jquery": 					"@$",
		"json2": 					"bower_components/json2/json2.js",
		"ejs": 						"lib/ejs/ejs.min.js",
		"moment": 					"bower_components/moment/min/moment.min.js",
		"moment-ru": 				"bower_components/moment/min/lang/ru.js",
		"bootstrap": 				"script/bootstrap.js",
		"Backbone": {
			"path": 				"bower_components/backbone/backbone-min.js",
			"require": {
				"underscore": 		"_",
				"jQuery": 			"$"
			},
			"exports": "Backbone"
		},
		"IframeTransport":			"bower_components/jquery-file-upload/js/jquery.iframe-transport.js",
		"jQueryWidget": {
			"path":					"bower_components/jquery-file-upload/js/vendor/jquery.ui.widget.js",
			"require": {
				"jQuery":			"$"
			}
		},		
		"FileUpload": {
			"path":					"bower_components/jquery-file-upload/js/jquery.fileupload.js",
			"require": {
				"jQuery":			"$"
			}
		},
		"cookie": {
			"path":					"bower_components/jquery.cookie/jquery.cookie.js",
			"require": {
				"jQuery": 			"$"        		
			}
		},
		"app": 						"script/app.js",
		"<%= file[0].toUpperCase() %><%= file.slice(1) %>": "script/{models,views,controllers,triggers}/**/*.js" 
	},
	"main": 						"app",
	"cache":						false,
	"ie":							true,
	"shortcuts": 					true,
	"pack":		 					false,
	"lazy":							false,
	"optimize":		 				false,
	"log": 							true,
	"warn": 						true

}