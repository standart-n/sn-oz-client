{
	"name": "sn build",
	"root": "../",
	"output": "script/sn.lmd.js",
	"modules": {
		"_": {
			"path": 				"lib/js/underscore.1.4.4.min.js",
			"exports": 				"_"
		},
		"underscore": 				"@_",
		"$": {
			"path": 				"lib/js/jQuery.1.8.3.min.js",
			"exports": 				"jQuery"
		},
		"jquery": 					"@$",
		"json2": 					"lib/js/json2.min.js",
		"ejs": 						"lib/js/ejs.min.js",
		"qunit": 					"lib/js/qunit.min.js",
		"bootstrap": 				"script/bootstrap.js",
		"Backbone": {
			"path": 				"lib/js/backbone.1.0.0.min.js",
			"require": {
				"underscore": 		"_",
				"jQuery": 			"$"
			},
			"exports": "Backbone"
		},
		"cookie": {
			"path":					"lib/js/jquery.cookie.min.js",
			"require": {
				"jQuery": 			"$"        		
			}
		},
		"app": 						"script/app.js",
		"test": 					"script/test.js",
		"<%= file[0].toUpperCase() %><%= file.slice(1) %>": "script/{models,views,controllers,triggers}/**/*.js" 
	},
	"main": 						"app",
	"shortcuts": 					true,
	"log": 							true,
	"pack":		 					false,
	"lazy":							false,
	"optimize":		 				false,
	"warn": 						true

}